<launch>
  <arg name="x_des" default="10" />
  <arg name="y_des" default="10" />
  <arg name="delay" default="2.0" />


  <param name="x_des" value="$(arg x_des)" />
  <param name="y_des" value="$(arg y_des)" />
  <group ns="crowd_navigation_core">
      <node pkg="crowd_navigation_core" type="send_actors_trajectory" name="send_actors_trajectory" output="screen" />
      <node pkg="crowd_navigation_core" type="send_desired_target_position" name="send_desired_target_position" launch-prefix="bash -c 'sleep $(arg delay); $0 $@' " />
  </group>
</launch>